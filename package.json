{
  "name": "p5.pave2riso",
  "version": "1.1.0",
  "description": "TypeScript library for converting Pave.js paths to Risograph print channels",
  "type": "module",
  "main": "./dist/p5.pave2riso.js",
  "types": "./dist/p5.pave2riso.d.ts",
  "exports": {
    ".": {
      "types": "./dist/p5.pave2riso.d.ts",
      "import": "./dist/p5.pave2riso.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && mv dist/index.js dist/p5.pave2riso.js && mv dist/index.d.ts dist/p5.pave2riso.d.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "pave",
    "pavejs",
    "risograph",
    "riso",
    "p5js",
    "creative-coding",
    "generative-art",
    "print",
    "typescript"
  ],
  "author": "hirookagikko <hirookagikko@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirookagikko/p5.pave2riso.git"
  },
  "bugs": {
    "url": "https://github.com/hirookagikko/p5.pave2riso/issues"
  },
  "homepage": "https://github.com/hirookagikko/p5.pave2riso#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@types/p5": "^1.7.0",
    "eslint": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "p5": "^1.9.0"
  }
}
